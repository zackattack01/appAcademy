<form id="post-form">
  <label for="title">Title:</label>
  <input type="text" id="title" name="post[title]" value="<%= post.get('title') %>">
  <br><br>

  <label for="body">Your Post:</label>
  <textarea id="body" name="post[body]"><%= post.get('body') %></textarea>
  <br>

  <button class="submit-post">Submit!</button>
</form>